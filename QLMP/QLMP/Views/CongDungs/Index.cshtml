
@model IEnumerable<QLMP.Models.CongDung>
@{
    ViewBag.Title = "Index";
}

<h2>CONG DUNG</h2>
<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.TenCongDung)
        </th>
        <th></th>
    </tr>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.TenCongDung)
            </td>
            <td>
                @Html.ActionLink("Edit", "Edit", new { id = item.MaCongDung }) |
                @Html.ActionLink("Details", "Details", new { id = item.MaCongDung }) |
                @Html.ActionLink("Delete", "Delete", new { id = item.MaCongDung })
            </td>
        </tr>
    }

</table>


